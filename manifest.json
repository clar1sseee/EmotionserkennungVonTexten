{
    "manifest_version": 3,
    "name": "Emotionserkennung von Texten",
    "version": "1.0.0",
    "author": "Clarisse Tiongson",
    "description": "Chrome-Extension zur Erkennung emotionaler Tonalit√§ten in Texten.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["https://*.example.com/*"],
